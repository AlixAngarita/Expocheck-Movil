{
  "expo": {
    "name": "Easy check",
    "slug": "Easy-check",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0abde3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.upb.easyCheck"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.upb.easycheck"
    },
    "notification": {
      "icon": "./assets/notificationIcon.png",
      "color": "#0abde3",
      "iosDisplayInForeground": true
    }
  }
}
